---
layout: null
---

<?xml version="1.0" encoding="UTF-8"?><rss version="2.0"
	xmlns:content="http://purl.org/rss/1.0/modules/content/"
	xmlns:wfw="http://wellformedweb.org/CommentAPI/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:atom="http://www.w3.org/2005/Atom"
	xmlns:sy="http://purl.org/rss/1.0/modules/syndication/"
	xmlns:slash="http://purl.org/rss/1.0/modules/slash/"
	>

<channel>
	<title>{{ site.title | xml_escape }}</title>
	<atom:link href="{{ "/feed.xml" | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/rss+xml" />
	<link>{{ site.url }}{{ site.baseurl }}</link>
	<description>{{ site.description | xml_escape }}</description>
	<lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
	<language>id-ID</language>
	<sy:updatePeriod>
	daily	</sy:updatePeriod>
	<sy:updateFrequency>
	1	</sy:updateFrequency>
	<generator>Jekyll v{{ jekyll.version }}</generator>

<image>
	<url>https://administrasi.net/favicon-32x32.png</url>
	<title>{{ site.title | xml_escape }}</title>
	<link>{{ site.url }}{{ site.baseurl }}</link>
	<width>32</width>
	<height>32</height>
</image> 
 {% for post in site.posts limit:30 %}
	<item>
		<title>{{ post.title | xml_escape }}</title>
		<link>{{ post.url | prepend: site.baseurl | prepend: site.url }}</link>
		
		<dc:creator><![CDATA[{{ post.author }}]]></dc:creator>
		<pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
				<category><![CDATA[{{ post.category | capitalize}}]]></category>
		<guid isPermaLink="false">{{ site.url }}{{ site.baseurl }}/{{ post.category | downcase }}</guid>

					<description><![CDATA[ <img="{%if post.image %}{{ site.url }}/img/{{ post.image | replace: '/img/','' }}{% else %}{{ site.url }}{{ site.logo }}{% endif %}" alt="{{ post.title | xml_escape }}" title="{{ post.title | xml_escape }}"></img>{{ post.content | truncate:350 }}
]]></description>
		
		
		
			</item>
		
		   {% endfor %}
	</channel>
</rss>
